<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin_preset name="PlugInEmbellishmentOptioneer">
	<cycle>2</cycle>
	<plugin_preset name="PlugInAddEmbellishmentRhythm">
		<chance>0.75</chance>
		<rhythm_options>
			<item>-1.0</item>
			<item>-1.5</item>
		</rhythm_options>
		<weighting>
			<item>0.5</item>
			<item>0.5</item>
		</weighting>
		<note_range>
			<mode>1</mode>
			<min>0</min>
			<max>127</max>
		</note_range>
	</plugin_preset>
	<plugin_preset name="PlugInAssignEmbellishment">
		<embellishment_options>
			<item>
				<type>d</type>
				<offset>1</offset>
			</item>
			<item>
				<type>d</type>
				<offset>2</offset>
			</item>
		</embellishment_options>
		<weighting>
			<item>0.5</item>
			<item>0.5</item>
		</weighting>
	</plugin_preset>
</plugin_preset>